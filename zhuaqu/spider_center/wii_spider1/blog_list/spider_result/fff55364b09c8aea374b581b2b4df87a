	        	var cookie = (function(){
					/**
					 * ∂¡»°cookie,◊¢“‚cookie√˚◊÷÷–≤ªµ√¥¯∆Êπ÷µƒ◊÷∑˚£¨‘⁄’˝‘Ú±Ì¥Ô ΩµƒÀ˘”–‘™◊÷∑˚÷–£¨ƒø«∞ .[]$  «∞≤»´µƒ°£
					 * @param {Object} cookieµƒ√˚◊÷
					 * @return {String} cookieµƒ÷µ
					 * @example
					 * var value = co.getCookie(name);
					 */
					var co={};
					co.getCookie = function (name) {
						var start = document.cookie.indexOf(name + "=");
				        var len = start + name.length + 1;
				        if ((!start) && (name != document.cookie.substring(0, name.length))) {
				            return null;
				        }
				        if (start == -1) return null;
				        var end = document.cookie.indexOf(';', len);
				        if (end == -1) end = document.cookie.length;
				        return unescape(document.cookie.substring(len, end));

					};

					/**
					 * …Ë÷√cookie
					 * @param {String} name cookie√˚
					 * @param {String} value cookie÷µ
					 * @param {Number} expire Cookie”––ß∆⁄£¨µ•Œª£∫–° ±
					 * @param {String} path ¬∑æ∂
					 * @param {String} domain ”Ú
					 * @param {Boolean} secure ∞≤»´cookie
					 * @example
					 * co.setCookie('name','sina',null,"")
					 */
					co.setCookie = function (name, value, expires, path, domain, secure) {
						var today = new Date();
				        today.setTime(today.getTime());
				        if (expires) {
				            expires = expires * 1000 * 60 * 60 * 24;
				        }
				        var expires_date = new Date(today.getTime() + (expires));
				        document.cookie = name + '=' + escape(value) +
				            ((expires) ? ';expires=' + expires_date.toGMTString() : '') +((path) ? ';path=' + path : '') +((domain) ? ';domain=' + domain : '') +((secure) ? ';secure' : '');
					};

					/**
					 * …æ≥˝cookie
					 * @param {String} name cookie√˚
					 */
					co.deleteCookie = function(name, path, domain) {
						if (co.getCookie(name)) document.cookie = name + '=' +((path) ? ';path=' + path : '') +((domain) ? ';domain=' + domain : '') +
			            ';expires=Thu, 01-Jan-1970 00:00:01 GMT';
					};
					return co;
				})();
				var splitStr = '|sEaRchHIS|';
				//À—À˜¿˙ ∑
				(function(){
					//≥ı ºªØ
					var creatDom = function(data){//¥¥Ω®dom
	        			var domStr='';
	        			var domArr = [];
	        			for(var i=0;i<data.length;i++){
