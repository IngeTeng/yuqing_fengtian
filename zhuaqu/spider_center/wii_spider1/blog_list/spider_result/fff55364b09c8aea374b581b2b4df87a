                            var itemStr = '<li><a href="javascript:;" class="liItem rssItem" data-qstr="'+encodeURIComponent(data[i])+'">'+data[i]+'</a><a class="delItem his_removeHis" href="javascript:;">…æ≥˝</a></li>';
		        			domArr.push(itemStr);
	        			}
	        			domStr = domArr.join('');
	        			return domStr;

	        		};
	        		if(getSearch()[1].length>0){
