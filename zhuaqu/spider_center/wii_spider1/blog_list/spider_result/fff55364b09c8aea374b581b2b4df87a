<!--[if lte IE 6]>
<script type="text/javascript">
	(function(){
		var inps = document.getElementsByTagName("input");
		for(i=0,len=inps.length;i<len;i++){
				if(inps[i].className.match("ipt-03")){
						inps[i].onmouseover = function(){
								this.className += " hover";
							}
						inps[i].onmouseout = function(){
								this.className =this.className.replace(/ hover/g,"");
							}	
					}
			}			
	})();
</script>
<![endif]--><script type="text/javascript">
    (function(){
        var $bluesReds = $( '.fblue, .fred', $('.wbret-tt') ),
            $links = $( '.wbret-link1' );

        $wblinks = $( 'span[s-role=wblink]' );
        $wblinks.click(function(e){
            e = e || event;
            window.open( $(this).attr('url') );
            if( e.preventDefault ) {
                e.preventDefault();
                e.stopPropagation();
            }
            else {
                e.returnValue = false;
                e.cancelBubble = true;
            }
        });

        $wblinks.bind( 'mouseenter', function(e){
            $( this ).css( { 'color': '#f00' } );
        });
        $wblinks.bind( 'mouseleave', function(e){
            $( this ).css( { 'color': '#26A2DA' } );
        });

        var hoverHandler = function( e ){
            e = e || event;
            var src = e.target || e.srcElement;

            if( src.className.indexOf('fred') >= 0 ) {
                $( src ).next( '.fblue' ).css( 'color', 'red' );
            }
            else if ( src.className.indexOf('fblue') >= 0 ) {
                $( src ).css( 'color', 'red' );
            }
            else if ( src.className.indexOf('wbret-link1') >= 0 ) {
                $( src ).css({
                    'color': '#4949b5',
                    'text-decoration': 'none'
                });
            }
        };

        var leaveHandler = function( e ) {
            e = e || event;
            var src = e.target || e.srcElement;

            if( src.className.indexOf('fred') >= 0 ) {
                $( src ).next( '.fblue' ).css( 'color', '#009' );
            }
            else if ( src.className.indexOf('fblue') >= 0 ) {
                $( src ).css( 'color', '#009' );
            }
            else if ( src.className.indexOf('wbret-link1') >= 0 ) {
                $( src ).css({
                    'text-decoration': 'none',
                    'color': '#7F7FCC'
                });
            }
        };

        $bluesReds.bind( 'mouseenter', hoverHandler );
        $links.bind( 'mouseenter', hoverHandler );

        $bluesReds.bind( 'mouseleave', leaveHandler );
        $links.bind( 'mouseleave', leaveHandler );

    })();
</script>










