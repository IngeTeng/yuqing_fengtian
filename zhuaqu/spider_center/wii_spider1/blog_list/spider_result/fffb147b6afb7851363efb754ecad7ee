                    t.et = this.nodeName;
                    t.x = e.pageX;
                    t.y = e.pageY;
                    t.uri = this.href || "";
                    if (i.href != undefined) {
                        var ji = $(i);
                        t.anchor = ji.text();
                        //t.anchor = void 0 === ji.data("title") ? ji.text() : ji.data("title");
