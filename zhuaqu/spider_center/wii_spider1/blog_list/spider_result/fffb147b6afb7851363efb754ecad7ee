                        last_sid = pre_sid;
                    }
                    //过滤掉链接内部元素点击重复发送的问题
                    else if (last_sid != pre_sid && $(i).closest('[href]').length == 0) {
                        t.anchor = '';
                        var s = new Image;
                        //getLocation();
                        s.src = Token.wrapURL(c + r(t));
                        last_sid = pre_sid;
                    }
                    //e.preventDefault();

                })
            })
    })(searchObj, clickObj);

