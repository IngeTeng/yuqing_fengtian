<script type="text/javascript" >
    function clickliSelect(id)
    {
        switch (id)
        {
            case 1:
                if (document.getElementById("sortli").style.display == "none")
                    document.getElementById("sortli").style.display = "block";
                else
                    document.getElementById("sortli").style.display = "none";
                
                if (document.getElementById("typeli") != undefined)
                    document.getElementById("typeli").style.display = "none";
                if (document.getElementById("timeli") != undefined)
                    document.getElementById("timeli").style.display = "none";
                if (document.getElementById("classli") != undefined)
                    document.getElementById("classli").style.display = "none";
                break;
            case 2:
                if (document.getElementById("timeli").style.display == "none")
                    document.getElementById("timeli").style.display = "block";
                else
                    document.getElementById("timeli").style.display = "none";

                if (document.getElementById("sortli") != undefined)
                    document.getElementById("sortli").style.display = "none";
                if (document.getElementById("typeli") != undefined)
                    document.getElementById("typeli").style.display = "none";
                if (document.getElementById("classli") != undefined)
                    document.getElementById("classli").style.display = "none";
                break;
            case 3:
                
                if (document.getElementById("typeli").style.display == "none")
                    document.getElementById("typeli").style.display = "block";
                else
                    document.getElementById("typeli").style.display = "none";
                
                if (document.getElementById("sortli") != undefined)
                    document.getElementById("sortli").style.display = "none";
                if (document.getElementById("timeli") != undefined)
                    document.getElementById("timeli").style.display = "none";
                if (document.getElementById("classli") != undefined)
                    document.getElementById("classli").style.display = "none";
                break;
            case 4:
                if (document.getElementById("classli").style.display == "none")
                    document.getElementById("classli").style.display =  "block";
                else 
                    document.getElementById("classli").style.display = "none";
                
                if (document.getElementById("sortli") != undefined)
                    document.getElementById("sortli").style.display = "none";
                if (document.getElementById("typeli") != undefined)
                    document.getElementById("typeli").style.display = "none";
                if (document.getElementById("timeli") != undefined)
                    document.getElementById("timeli").style.display = "none";
                break;
        }
    }
    function clickopendiv()
    {      
        if (document.getElementById("screenul") != undefined)
        {
            if (document.getElementById("screenul").style.display == "none") {
                document.getElementById("screenul").style.display = "block";
                document.getElementById("searchcount").style.display = "none";
            } else {
                document.getElementById("screenul").style.display = "none";
                document.getElementById("searchcount").style.display = "block";
            }
        }
    }
    
    //失去焦点隐藏
    window.onload = function () {
        if (document.addEventListener) {
            document.addEventListener('click', callback, false);
        }else if(document.attachEvent)
        {
            document.attachEvent('onclick', callback);
        } else
        {
            document.onclick = callback;
        }
       
    };

    function callback(evt)
    {
        evt = evt || window.event;
        var dom1 = (typeof evt.target !== 'undefined') ? evt.target : evt.srcElement;
        var sortli = document.getElementById("sortli");
        var timeli = document.getElementById("timeli");
        var typeli = document.getElementById("typeli");
        var classli = document.getElementById("classli");
        if (sortli != null) {
            if ((dom1.id != "sortsp" && dom1.id != "sorti") && document.getElementById("sortli").style.display == "block") {
                document.getElementById("sortli").style.display = "none";
            }
        }
        if (timeli != null) {
            if ((dom1.id != "timesp" && dom1.id != "timei") && document.getElementById("timeli").style.display == "block") {
                document.getElementById("timeli").style.display = "none";
            }
        }
        if (typeli != null) {
            if ((dom1.id != "typesp" && dom1.id != "typei") && document.getElementById("typeli").style.display == "block") {
                document.getElementById("typeli").style.display = "none";
            }
        }
        if (classli != null) {
            if ((dom1.id != "classsp" && dom1.id != "classi") && document.getElementById("classli").style.display == "block") {
                document.getElementById("classli").style.display = "none";
            }
        }
        
    }
</script>
                
                

                

                
            </div>
