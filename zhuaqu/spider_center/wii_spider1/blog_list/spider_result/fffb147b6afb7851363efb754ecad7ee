<script>
    (function() {
        var floatHead = $('.ss-header').eq(0);
        var floatTop  = floatHead.offset().top;
        var floatHt   = floatHead.height();
        var floatNext = floatHead.next();
        $(window).on('scroll', function(e) {
            var scrollTop = $(this).scrollTop();
            if (scrollTop > floatTop) {
                floatHead.css({position:'fixed',top:0,left:0,width:'100%'});
                floatNext.css({marginTop:floatHt + 'px'});
            } else {
                floatHead.css({position:'static'});
                floatNext.css({marginTop:0});
            }
        });
    })();
</script>

<script>
