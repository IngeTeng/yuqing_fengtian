<script type="text/javascript">
    ; (function () {

        function getCoords(elem) {
            var box = elem.getBoundingClientRect(),
            doc = elem.ownerDocument,
            body = doc.body,
            html = doc.documentElement,
            clientTop = html.clientTop || body.clientTop || 0,
            clientLeft = html.clientLeft || body.clientLeft || 0,
            top = box.top + (self.pageYOffset || html.scrollTop || body.scrollTop) - clientTop,
            left = box.left + (self.pageXOffset || html.scrollLeft || body.scrollLeft) - clientLeft
            return { 'top': top, 'left': left };
        };

        function addEvent(elem, type, fn) {
            elem.addEventListener ? elem.addEventListener(type, fn, false) : elem.attachEvent('on' + type, fn);
        };

        function scrollBtn(rid, sid, tid) {
            var r_obj = document.getElementById(rid),
                s_obj = document.getElementById(sid),
                t_obj = document.getElementById(tid),
                sCss = s_obj.style, tCss = t_obj.style;

            var isFixed = (function () {

                var outer = document.createElement('div'),
                    inner = document.createElement('div'),
                    result = true;

                outer.style.cssText = 'position:absolute;top:200px;';
                inner.style.cssText = 'position:fixed;top:100px;';

                outer.appendChild(inner);
                document.body.appendChild(outer);

                if (inner.getBoundingClientRect &&
                    inner.getBoundingClientRect().top == outer.getBoundingClientRect().top) result = false;

                document.body.removeChild(outer);

                outer = inner = null;

                return result;

            })();

            var set = function () {
                var st = document.documentElement.scrollTop + document.body.scrollTop,
                    wH = document.documentElement.clientHeight,
                    rPos = getCoords(r_obj);

                if (st + wH - 48 > rPos.top + r_obj.offsetHeight) {
                    sCss.position = 'absolute';
                    sCss.bottom = '';
                    sCss.top = rPos.top + r_obj.offsetHeight - 94 + 'px';
                    if(st == 0) sCss.height = '39px';
                    else sCss.height = '80px';
                }
                else {
                    sCss.position = isFixed ? 'fixed' : 'absolute';
                    if (st > 0) {
                        sCss.bottom = '';
                        sCss.top = (isFixed ? 0 : st) + wH - 150 + 'px';
                        tCss.display = '';
                        sCss.height = '80px';
                    }
                    else {
                        sCss.height = '39px';
                        sCss.top = '';
                        sCss.bottom = '70px';
                    }
                }
            };

            setInterval(function () {
                set();
            }, 50);
        };

        scrollBtn('content', 'scrollbtn', 'turntop');
    })();

</script>


