		$('.ina_tz_input').keyup(function(e){
			var ev = e ? e : window.event;
			if(e && e.keyCode==13){
	            var value=$(".ina_tz_input").val();
	            page = 0;
		        if(value=='')
		        {
		            return false;
		        }
		        if(value>page)
		        {
		            value=page;
		        }
	        	//var url = '/news/more_list_column_35_{$page}.html';
