<script type="text/javascript">
	function submitform(_name){
		var val = document.getElementById(_name).value;
		var host = 'http://' + window.location.hostname;
		if (val.length>0) {
			var newUrl = host +'/search/index/'+val+'/';
		}else{
			var newUrl = host +'/search/';
		}
		
		window.location.href = newUrl;
	}

	$(function(){
		/* 文本框值改变时更新列表，判断键值，执行事件  */
		var tipNum = 0;
		$('.search_text').keyup(function(e){
			var ev = e ? e : window.event;
			var _id = $(this).attr('id'),
				val = $(this).val(),
				obj = $('#search_name'),
				tip = $('#ina_similar');				
			// 顶部
			if( _id == 'search_name' ){										
				if(e && e.keyCode==13){
					submitform( 'search_name' );
				}
				if( e.keyCode == 38 || e.keyCode == 40 ){
					if ( $('#ina_similar').is(':visible') ){
						var max = tip.find('li').length;
						if( e.keyCode == 38 ){ // 上键
							tipNum--;
							if( tipNum <= 0 ){
								tipNum = max;
							}
						}
						if( e.keyCode == 40 ){ // 下键
							tipNum++;
							if( tipNum > max ){
								tipNum = 1;
							}
						}
						var current = tip.find('li').eq( tipNum - 1 );
						current.css({'background':'#ccc'}).siblings().css({'background':'#fff'});
						obj.val( current.find('span').text() );
					}
				}
				if ( e.keyCode != 38 && e.keyCode != 40 ){
					keyword_guess(val);
					tipNum = 0;
				}

			}else if( _id == 'search_nameBot' ){
				if(e && e.keyCode==13){
					submitform('search_nameBot');
				}
			}
		})
		

		$('#search_name').on('focus',function(){
			var val = $(this).val();
			keyword_guess(val);
		})
		document.onclick = function (e) {
		    e = e || window.event;
		    var dom = e.srcElement || e.target;
		    var objword = document.getElementById("ina_similar");
		    if (objword != null) {
		        if (dom.id != "ina_similar" && dom.id != "search_name" && document.getElementById("ina_similar").style.display == "block") {
		            	document.getElementById("ina_similar").style.display = "none";
		        }
		    }
		};
		function keyword_guess(msg){
			if (msg == '') {
				return false;
			}
			$.ajax({
				url:'/init.php?m=search&c=search&a=down_prompt',
				type:'get',
				cache: false,
				data:{ keyword:msg },
				success:function(mess){
					if(mess != ''){
	             		mess=eval("(" + mess + ")");
	             		console.log(mess.isHaveData);
	             		if (mess.isHaveData != 0) {

				            var htmlstr='';
				            console.log(mess);
				            if( mess.top ){
								if(mess.top.length>0){
									$.each(mess.top, function (key, val) {
										if(val && val.otherid!=null && val.name!=null && val.price!=null){
											htmlstr+="<li><a href=\"/search/index/"+val.name+"/\" target=\"_self\"><span>"+val.name+'</span><i>'+val.price+'</i></a></li>';			
										}
									})
					            }
				            }
				            if(mess.bottom.length>0){
				               $.each(mess.bottom, function (key, val) {
				               		if(val && val.otherid!=null && val.name!=null && val.price!=null){
										htmlstr+="<li><a href=\"/"+val.otherid+"/\" target=\"_self\"><span>"+val.name+'</span><i>'+val.price+'</i></a></li>';
									}
								})
				            }
	                		document.getElementById('ina_similar').innerHTML=htmlstr;
							$('.ina_similar').show();
						}
					}else{
						$('.ina_similar').hide();
					}
				}
			})
		}


	})
	
</script>

